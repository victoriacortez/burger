<h1>Burgers</h1>

<h3>Create a burger</h3>

<ul>
  {{#each burgers}}
  	<li>
  	   <p>id: {{this.id}}</p>
       <p>Event: {{this.burger_name}}</p>
  	</li>
  {{/each}}
</ul>

<form action="/" method="POST">
	<textarea type="text" name="food"></textarea>

	<button type="submit">Submit</button>
	<button type="delete">Devour</button>
</form>
